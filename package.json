{
  "name": "mizzy",
  "version": "0.1.0",
  "description": "A MIDI Processing Library",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwmwmw/Mizzy.git"
  },
  "type": "module",
  "keywords": [
    "Midi",
    "Music"
  ],
  "contributors": [
    "Matthew Willox <matthew@cybermancers.ca> (https://github.com/mwmwmw)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwmwmw/Mizzy/issues"
  },
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --emitDeclarationOnly",
    "watch": "tsc --watch",
    "test": "jest",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/mwmwmw/Mizzy#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
